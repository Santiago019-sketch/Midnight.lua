local players = game:GetService("Players")
local replicatedStorage = game:GetService("ReplicatedStorage")
local workspace = game:GetService("Workspace")
local localPlayer = players.LocalPlayer
local playerName = localPlayer.Name

-- Define tables
local tbl_main2 = {
    localPlayer.Stats,
    localPlayer.Status,
    workspace[playerName].Humanoid,
    workspace[playerName].RightHand
}

local tbl_main = {
    localPlayer.Status,
    localPlayer.Stats,
    workspace[playerName].HumanoidRootPart,
    workspace[playerName].RightHand,
    workspace[playerName]
}

local tbl_0xa51df21d2a5b4043 = {
    ["LeftLowerArm"] = workspace[playerName].LeftLowerArm,
    ["RightUpperArm"] = workspace[playerName].RightUpperArm,
    ["LeftFoot"] = workspace[playerName].LeftFoot,
    ["RightHand"] = workspace[playerName].RightHand,
    ["RightLowerArm"] = workspace[playerName].RightLowerArm,
    ["LeftUpperLeg"] = workspace[playerName].LeftUpperLeg,
    ["LeftUpperArm"] = workspace[playerName].LeftUpperArm,
    ["Character"] = workspace[playerName],
    ["LeftHand"] = workspace[playerName].LeftHand,
    ["RightFoot"] = workspace[playerName].RightFoot,
    ["Humanoid"] = workspace[playerName].Humanoid,
    ["RightLowerLeg"] = workspace[playerName].RightLowerLeg,
    ["RightUpperLeg"] = workspace[playerName].RightUpperLeg,
    ["LeftLowerLeg"] = workspace[playerName].LeftLowerLeg
}

local tbl_main1 = {
    localPlayer.Stats,
    tbl_0xa51df21d2a5b4043
}

local tbl_main6 = {
    localPlayer.Stats
}

-- Optimize the loop
local runService = game:GetService("RunService")
runService.RenderStepped:Connect(function()
    replicatedStorage.Remotes.Training.Combat5:InvokeServer(unpack(tbl_main1))
    replicatedStorage.Remotes.Training.Defense5:InvokeServer(unpack(tbl_main2))
    replicatedStorage.Remotes.Training.Blast5:InvokeServer(unpack(tbl_main))
    replicatedStorage.Remotes.Training.Agility5:FireServer(unpack(tbl_main6))
end)
